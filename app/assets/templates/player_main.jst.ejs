<button type="button" class="btn btn-default btn-lg">
  Pause
</button>

<button type="button" class="btn btn-default btn-lg">
  Play
</button>

<button type="button" class="btn btn-default btn-lg">
  Stop
</button>

<button type="button" class="btn btn-default btn-lg">
  <% if (track.id) {%>
    Currently Playing: &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;  <strong><%= track.get('title') %></strong>
  <%}else{  %>
    Currently Playing: &nbsp;&nbsp;&nbsp;&nbsp;&nbsp; Nothing is playing.
  <%} %>
</button>

<button type="button" class="btn btn-primary btn-lg" id="focus-mode-switch">
  <% if (window.focused) { %>
    Exit Focus Mode
  <% }else{ %>
    Focus Mode
  <% } %>
</button>

